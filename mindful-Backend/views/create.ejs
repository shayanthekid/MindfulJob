<body>

   <%- include('./partials/nav.ejs')  %>
<div class="create-blog content">

<form action="/events" method="POST">
<label for="title">Blog title:</label>
<input type="text" id="title" name="title" required>
<label for="snippet">Date :</label>
<input type="date" id="date" name="date" required>
<label for="snippet">Venue Start :</label>
<input type="number" id="venueStart" name="venueStart" required>
<label for="snippet">Venue End :</label>
<input type="number" id="venueEnd" name="venueEnd" required>
<label for="snippet">Seats:</label>
<input type="number" id="seats" name="seats" required>
<label for="snippet">Price:</label>
<input type="number" id="price" name="price" required>
<button>Submit</button>
</form>

</div>



